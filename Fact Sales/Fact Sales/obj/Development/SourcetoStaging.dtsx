                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    SD.SalesOrderDetailID AS SalesOrderDetailKey,
    SH.SalesOrderID AS SalesOrderKey,
    P.ProductID AS ProductKey,
    SH.Status AS SalesOrderStatusKey,
    SH.OnlineOrderFlag AS SalesOnlineOrderFlag,
    C.CustomerID AS SalesOrderCustomerKey,
    ISNULL(SP.BusinessEntityID, 0) AS SalesPersonKey,
    ST.TerritoryID AS TerritoryKey,
    A.AddressID AS BillToAddressKey,
    CONVERT(INT, CONVERT(VARCHAR(8), SH.OrderDate, 112)) AS SalesOrderDateKey,
    CONVERT(INT, CONVERT(VARCHAR(8), SH.DueDate, 112)) AS SalesOrderDueDateKey,
    SD.OrderQty,
    SD.UnitPrice,
    SD.UnitPriceDiscount,
    SD.LineTotal,
    SH.SubTotal AS SalesOrderSubTotal,
    SH.TaxAmt AS SalesOrderTaxAmount,
    SH.Freight AS SalesOrderFreightAmount,
    SH.TotalDue AS SalesOrderTotalDueAmount,
    SH.ModifiedDate AS SalesOrderModifiedDateKey
FROM Sales.SalesOrderDetail SD
    LEFT JOIN Sales.SalesOrderHeader SH ON SD.SalesOrderID = SH.SalesOrderID
    LEFT JOIN Sales.Customer C ON SH.CustomerID = C.CustomerID
    LEFT JOIN HumanResources.Employee SP ON SH.SalesPersonID = SP.BusinessEntityID
    LEFT JOIN Sales.SalesTerritory ST ON SH.TerritoryID = ST.TerritoryID
    LEFT JOIN Production.Product P ON SD.ProductID = P.ProductID
    LEFT JOIN Person.Address A ON SH.BillToAddressID = A.AddressID;</property>
